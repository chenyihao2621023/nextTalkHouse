{
    "presets": [
        [
            "env",
            {
                "modules": false,
                "targets": {
                  "browsers": ["> 10%", "last 20 versions", "not ie <= 8"]
                }
            }
        ],
        "stage-2",
        "es2015",
        "react"
    ],
    "plugins": [
        "transform-runtime",
        "transform-decorators-legacy",
        "transform-class-properties",
        ["import", { "libraryName": "antd", "style": "css" }]
    ],
    "comments": false,
    "env": {
        "test": {
            "presets": [
                "env",
                "stage-2"
            ],
            "plugins": [
                "istanbul"
            ]
        }
    }
}
